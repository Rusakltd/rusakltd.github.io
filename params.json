  "body": "<!DOCTYPE html>\r\n<html lang=\"ru\">\r\n    <head>\r\n        <meta charset=\"utf-8\">\r\n        <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n        <title>Fun site Overwatch</title>\r\n        <meta name=\"description\" content=\"Фан.\">\r\n        <link rel=\"stylesheet\" href=\"main.css\">\r\n    </head>\r\n    <body>\r\n        <header class=\"main-header\">\r\n            <div class=\"container\">\r\n                <nav class=\"main-navigation\">\r\n                    <ul>\r\n                        <li>\r\n                            <a href=\"#\">Information</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"#\">News</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"#\">Price-List</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"#\">Market</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"#\">Contacts</a>\r\n                        </li>\r\n                    </ul>\r\n                </nav>\r\n                <div class=\"user-block\">\r\n                    <a class=\"login\" href=\"#\">Login</a>\r\n                </div>\r\n            </div>\r\n        </header>\r\n\r\n        <main class=\"conteiner\">\r\n            <div class=\"index-logo\">\r\n                <img src=\"img/index-logo.png\" width=\"368\" height=\"204\" alt=\"Barbershop\">\r\n            </div>\r\n            <script src=\"https://cdnjs.cloudflare.com/ajax/libs/lodash.js/4.3.0/lodash.js\"></script>\r\n            <section class=\"features\">\r\n                <div class=\"feautures-item\">\r\n                    <b class=\"feautures-name\">COOL</b>\r\n                    <p>Proffesionals</p>\r\n                </div>\r\n                <div class=\"feautures-item\">\r\n                    <b class=\"feautures-name\">C